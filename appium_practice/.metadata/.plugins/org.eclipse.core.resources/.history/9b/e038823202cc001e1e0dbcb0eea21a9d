package demo4java;
class B{
	B(){
		System.out.print("constructor called....");
	}
}
class employee{
	int empId;
	String empname;
	float salary;
	
	public employee() {
		// TODO Auto-generated constructor stub
		empId=1001;
		empname = "yash";
		salary=50000;
	}
	void print_data() {
		System.out.println("employee id " + empId + "\nemployee name " + empname + "employee salary " + salary);;
	}
}

public class second {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			B obj = new B();
			employee e1 = new employee();
			e1.print_data();
	}

}
